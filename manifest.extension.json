{
  "manifest_version": 3,
  "name": "ClearTab - New Tab",
  "version": "1.0.0",
  "description": "The beautiful and powerful new tab page.",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "identity"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}